{
    "name": "@gmx-io/sdk",
    "version": "0.0.1",
    "type": "module",
    "main": "build/index.js",
    "exports": {
        ".": {
            "import": "./build/index.js",
            "require": "./build/index.js"
        },
        "./abis/*": "./build/abis/*",
        "./prebuilt/*": "./build/prebuilt/*",
        "./utils/*": "./build/utils/*"
    },
    "scripts": {
        "prebuild": "yarn tsx scripts/prebuild",
        "prepare": "ts-patch install -s",
        "copy:jsons": "cp -r src/abis build && cp -r src/prebuilt build",
        "test": "jest",
        "tscheck": "tsc -p tsconfig.json --noEmit",
        "build": "rm -rf build && tsc -p tsconfig.json && yarn copy:jsons"
    },
    "dependencies": {
        "cross-fetch": "4.0.0",
        "graphql": "15.8.0",
        "isomorphic-performance": "5.1.1",
        "lodash": "4.17.21",
        "query-string": "7.1.1",
        "typescript": "5.4.2",
        "universal-perf-hooks": "1.0.1",
        "viem": "2.7.20"
    },
    "jest": {
        "preset": "ts-jest/presets/default-esm",
        "testEnvironment": "node",
        "testPathIgnorePatterns": [
            "/build/",
            "/node_modules/"
        ],
        "moduleFileExtensions": [
            "ts",
            "js",
            "json"
        ],
        "moduleNameMapper": {
            "^configs/(.*)$": "<rootDir>/src/configs/$1",
            "^modules/(.*)$": "<rootDir>/src/modules/$1",
            "^utils/(.*)$": "<rootDir>/src/utils/$1",
            "^types/(.*)$": "<rootDir>/src/types/$1",
            "prebuilt": "<rootDir>/src/prebuilt/index.ts",
            "^abis/(.*)$": "<rootDir>/src/abis/$1"
        }
    },
    "eslintConfig": {
        "rules": {
            "no-console": "warn"
        }
    },
    "engines": {
        "node": ">=18.0.0"
    },
    "devDependencies": {
        "@babel/plugin-proposal-private-property-in-object": "7.21.11",
        "@types/jest": "29.5.13",
        "@types/lodash": "4.14.198",
        "@types/node": "18.7.13",
        "@typescript-eslint/eslint-plugin": "6.5.0",
        "@typescript-eslint/parser": "6.5.0",
        "eslint": "^8.41.0",
        "eslint-config-prettier": "9.0.0",
        "eslint-config-recommended": "4.1.0",
        "eslint-plugin-local-rules": "2.0.1",
        "husky": "7.0.4",
        "jest": "29.7.0",
        "lint-staged": "12.3.4",
        "prettier": "3.2.5",
        "ts-jest": "29.2.5",
        "ts-patch": "3.2.1",
        "tsx": "4.19.0",
        "typescript-transform-paths": "3.5.1"
    },
    "lint-staged": {
        "src/**/*.{js,ts,jsx,tsx}": [
            "eslint --fix --max-warnings=0 ./src",
            "prettier --write"
        ],
        "src/**/*.{css,scss}": [
            "prettier --write"
        ]
    },
    "files": [
        "build"
    ],
    "packageManager": "yarn@3.1.0"
}
